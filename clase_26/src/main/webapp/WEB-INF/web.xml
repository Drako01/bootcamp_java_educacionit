<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">

	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>InicioServlet</servlet-name>
		<display-name>InicioServlet</display-name>
		<description></description>
		<servlet-class>com.educacionit.InicioServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AgregarAlumnoServlet</servlet-name>
		<display-name>AgregarAlumnoServlet</display-name>
		<description></description>
		<servlet-class>com.educacionit.AgregarAlumnoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>BorrarAlumnoServlet</servlet-name>
		<display-name>BorrarAlumnoServlet</display-name>
		<description></description>
		<servlet-class>com.educacionit.BorrarAlumnoServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>LimpiarSessionServlet</servlet-name>
		<display-name>LimpiarSessionServlet</display-name>
		<description></description>
		<servlet-class>com.educacionit.LimpiarSessionServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>InicioServlet</servlet-name>
		<url-pattern>/InicioServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AgregarAlumnoServlet</servlet-name>
		<url-pattern>/AgregarAlumnoServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>BorrarAlumnoServlet</servlet-name>
		<url-pattern>/BorrarAlumnoServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>LimpiarSessionServlet</servlet-name>
		<url-pattern>/LimpiarSessionServlet</url-pattern>
	</servlet-mapping>
</web-app>
